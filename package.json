{
  "name": "one-piece-data",
  "description": "Get random names and other stuff from the Japanese anime One Piece",
  "main": "dist/index.js",
  "scripts": {
    "precommit": "npm test",
    "prepush": "npm test",
    "commit": "git-cz",
    "watch:test": "npm run test -- -w",
    "test": "mocha src/index.test.js --compilers js:babel-register",
    "prebuild": "rimraf dist",
    "build:main": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p",
    "build": "npm-run-all --parallel build:*",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tibuurcio/one-piece-data.git"
  },
  "keywords": [
    "random",
    "onepiece",
    "anime",
    "japanese"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Gabriel Tib√∫rcio <tibuurcio@gmail.com> (https://tibuurcio.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tibuurcio/one-piece-data/issues"
  },
  "homepage": "https://github.com/tibuurcio/one-piece-data#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "4.0.0",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "husky": "0.13.4",
    "json-loader": "0.5.4",
    "mocha": "3.4.2",
    "npm-run-all": "4.0.2",
    "rimraf": "2.6.1",
    "semantic-release": "^6.3.6",
    "webpack": "2.6.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  }
}

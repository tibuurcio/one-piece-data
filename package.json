{
  "name": "one-piece-data",
  "version": "1.1.1",
  "description": "Get random names and other stuff from the Japanese anime One Piece",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha src/index.test.js -w",
    "test:single": "mocha src/index.test.js",
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tibuurcio/one-piece-data.git"
  },
  "keywords": [
    "random",
    "onepiece",
    "anime",
    "japanese"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Gabriel Tib√∫rcio <tibuurcio@gmail.com> (https://tibuurcio.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tibuurcio/one-piece-data/issues"
  },
  "homepage": "https://github.com/tibuurcio/one-piece-data#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chai": "4.0.0",
    "ghooks": "2.0.0",
    "mocha": "3.4.2",
    "rimraf": "2.6.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single"
    }
  },
  "babel": {
    "presets": ["es2015", "stage-2"]
  }
}
